        <activity android:name="com.supcon.mes.module_beacon_no_login.beaconManage.DialogMapActivity"
            android:theme="@style/customDialogStyle"
            />
        <service
            android:name="com.tencent.smtt.export.external.DexClassLoaderProviderService"
            android:label="dexopt"
            android:process=":dexopt" />

        <activity android:name="com.supcon.mes.module_beacon_no_login.powerSetting.DeviceControlActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar"/>

        <service
            android:name="com.supcon.mes.module_beacon_no_login.service.BluetoothLeService"
            android:enabled="true" />
    </application>

</manifest>