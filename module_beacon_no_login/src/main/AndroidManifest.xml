<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.supcon.supbeacon">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity android:name="com.supcon.mes.module_beacon_no_login.rssiCorrection.BluetoothDetailActivity"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name="com.supcon.mes.module_beacon_no_login.powerSetting.DeviceListForScanActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.powerSetting.PowerSettingActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.HomeActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.ui.JSInteractiveActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.ui.GPSActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.SplashActivity">
        </activity>
        <activity android:name="com.supcon.mes.module_beacon_no_login.BaseActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.rssiCorrection.RssiCorrectionActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.powerSetting.DeviceScanActivity" />
        <activity android:name="com.supcon.mes.module_beacon_no_login.beaconManage.BeaconDetailActivity"/>
