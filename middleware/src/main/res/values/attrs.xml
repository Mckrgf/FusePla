<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="title" format="string" />
    <attr name="value" format="string" />
    <attr name="hint" format="string" />
    <attr name="icon" format="reference" />
    <attr name="color" format="color" />

    <declare-styleable name="DifffrentColorTextView">
        <attr name="textone" format="string" />
        <attr name="colorone" format="color" />
        <attr name="texttwo" format="string" />
        <attr name="colortwo" format="color" />
        <attr name="textsizeone" format="integer"/>
        <attr name="textsizetwo" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTitleValueSmall">
        <attr name="title"/>
        <attr name="value"/>
        <attr name="icon"/>
    </declare-styleable>

    <declare-styleable name="CustomTitleValueDetail">
        <attr name="title"/>
        <attr name="value"/>
        <attr name="icon"/>
        <attr name="fixedline" format="boolean"/>
        <attr name="lineneedshow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomGroupTitle">
        <attr name="title"/>
        <attr name="icon"/>
        <attr name="isopen" format="boolean" />
        <attr name="hasicon" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CustomTitleValueEdit">
        <attr name="title"/>
        <attr name="value"/>
        <attr name="hint"/>
        <attr name="edit_type" format="integer" />
        <attr name="isrequired" format="boolean" />
        <attr name="is_need_show_arrow" format="boolean" />
        <attr name="is_current_page_edit" format="boolean" />
        <attr name="maxlength" format="integer" />
        <attr name="maxmoney" format="float" />
        <attr name="digits" format="string"></attr>
    </declare-styleable>

    <declare-styleable name="CustomTextState">
        <attr name="value"/>
        <attr name="color"/>
    </declare-styleable>

    <declare-styleable name="CustomEditWithCount">
        <attr name="value"/>
        <attr name="hint"/>
        <attr name="maxcount" format="integer" />
    </declare-styleable>

    <declare-styleable name="CustomTitleValueSmallTop">
        <attr name="titleleft" format="string" />
        <attr name="valueleft" format="string" />
        <attr name="titleright" format="string" />
        <attr name="valueright" format="string" />
    </declare-styleable>

    <item type="id" name="drag_handle" />
    <item type="id" name="click_remove" />
    <item name="first" type="id" />

    <declare-styleable name="WarpLinearLayout">
        <attr name="grivate" format="enum"><!--对齐方式 !-->
            <enum name="right" value="0"></enum>
            <enum name="left" value="1"></enum>
            <enum name="center" value="2"></enum>
        </attr>
        <attr name="horizontal_Space" format="dimension"></attr>
        <attr name="vertical_Space" format="dimension"></attr>
        <attr name="isFull" format="boolean"></attr>
    </declare-styleable>
    <style name="WarpLinearLayoutDefault">
        <item name="grivate">left</item>
        <item name="horizontal_Space">20dp</item>
        <item name="vertical_Space">20dp</item>
        <item name="isFull">false</item>
    </style>
    <declare-styleable name="TagFlowLayout">
        <attr name="max_select" format="integer"/>
        <attr name="tag_gravity">
            <enum name="left" value="-1" />
            <enum name="center" value="0" />
            <enum name="right" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="SimpleSwitchView">
        <attr name="values" format="string"/>
        <attr name="text_width" />
        <attr name="text" />
        <attr name="text_size" />
        <attr name="text_color" />
        <attr name="necessary" />
        <attr name="enable" />
        <attr name="editable" />
    </declare-styleable>

    <declare-styleable name="CustomMiddleFilterView">
        <attr name="text"/>
        <attr name="text_color"/>
        <attr name="text_size"/>
        <attr name="position"/>
        <attr name="icon_res"/>
        <attr name="item_text_color"/>
        <attr name="item_bg_color"/>
        <attr name="item_text_size"/>
        <attr name="num"/>
    </declare-styleable>
    <declare-styleable name="FileListView">
        <attr name="isShow" format="boolean" />
    </declare-styleable>
    <declare-styleable name="CustomMiddleSearchHistoryView">
        <attr format="string" name="search_hint"/>
        <attr format="integer" name="search_text_size"/>
        <attr format="color" name="search_text_color"/>
        <attr format="color" name="search_text_bg_color"/>
        <attr format="color" name="search_bg_color"/>
    </declare-styleable>

    <declare-styleable name="MaxHeightView">
        <attr name="mhv_HeightRatio" format="reference|float" />
        <attr name="mhv_HeightDimen" format="reference|dimension" />
    </declare-styleable>

    <declare-styleable name="CustomSearchTitleView">
        <!--圆角或者平角角度 -->
        <attr name="custom_Radio" format="float|dimension"/>
    </declare-styleable>

    <declare-styleable name="CustomTabView">
        <attr name="title"/>
        <attr name="countstring" format="string"/>
        <attr name="leftneedshow" format="boolean"/>
        <attr name="rightneedshow" format="boolean"/>
    </declare-styleable>

</resources>